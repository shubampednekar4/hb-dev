{
    "name": "your-name-here/material-bake",
    "description": "MaterialBake plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MaterialBake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaterialBake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
